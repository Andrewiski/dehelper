{
    "name": "@andrewiski/dehelper",
    "version": "0.0.1",
    "description": "Node.js Helper Library Including User Auth Middleware with Bootstrap UI",
    "main": "index.js",
    "license": "GPL-3.0-or-later",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/andrewiski/dehelper"
    },
    "author": {
        "name": "Andrew DeVries",
        "email": "adevries@digitalexample.com",
        "url": "https://digitalexample.com"
    },
    "engines": {
        "node": ">=16.0"
    },
    "scripts": {
        "compile": "rimraf ./dist && tsc",
        "test": "npm run format:check && npm run compile && npm run test:types && npm run test:unit",
        "test:types": "tsd",
        "test:unit": "nyc mocha --require ts-node/register --reporter spec --slow 200 --bail --timeout 10000 test/index.ts",
        "format:check": "prettier --check \"lib/**/*.ts\" \"test/**/*.ts\"",
        "format:fix": "prettier --write \"lib/**/*.ts\" \"test/**/*.ts\"",
        "prepack": "npm run compile"
    },
    "dependencies": {
        "debug": "^4.3.2",
        "express": "^4.17.13",
        "extend": "^3.0.2",
        "nosql": "^6.1.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/debug": "^4.1.8",
        "@types/express": "^4.17.13",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.11.7",
        "expect.js": "0.3.1",
        "mocha": "^10.0.0",
        "nyc": "^15.1.0",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "superagent": "^8.0.0",
        "supertest": "^6.1.6",
        "ts-node": "^10.2.1",
        "tsd": "^0.21.0",
        "typescript": "^4.4.2"
    }
}
